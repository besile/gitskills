<?xml version="1.0"?>
<configuration>
  <connectionStrings>
    <add name="Humor" connectionString="server=.;uid=sa;pwd=xubinghua2010.;database=Humor;pooling=true;min pool size=1;max pool size=50"/>
  </connectionStrings>
  <appSettings>
    <!--缓存实体配置文件路径-->
    <add key="CacheCaseFilePath" value="/CacheCase.xml" />
